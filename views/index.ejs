            <% if(IsAdmin==='didi' ) {%>

                <p class="mt-4"><a href="/dashboard/new">Tambah Agenda Baru</a></p>
                <% } %>
                    <p class="mb-4"></p>
                    <% if(success && success.length) {%>
                        <div class="alert alert-success alert-dismissible fade show mt-4" role="alert">
                            <%= success %>
                                <button type="button" class="btn-close" data-bs-dismiss="alert"
                                    aria-label="Close"></button>
                        </div>
                        <% } %>
                            <% if(dataAgenda) {%>
                                <table
                                    class="table table-dark table-striped table-bordered border-primary table-sm mb-5 table-hover css-serial">
                                    <thead>
                                        <tr>
                                            <th scope="col" class="text-center align-middle">Date</th>
                                            <th scope="col" class="text-center align-middle">Month</th>
                                            <th scope="col" class="text-center align-middle">Time</th>
                                            <th scope="col" class="text-center align-middle">Daily Agenda</th>
                                            <th scope="col" class="text-center align-middle">Category</th>
                                            <th scope="col" class="text-center align-middle">STATUS</th>
                                            <!-- <th scope="col" class="text-center align-middle">Katerangan Hasil</th>
                                        <th scope="col" class="text-center align-middle">Katerangan Final</th> -->
                                            <% if(IsAdmin !='guest' ) {%>

                                                <th scope="col" class="text-center align-middle">Edit</th>
                                                <th scope="col" class="text-center align-middle">Hapus</th>
                                                <% } %>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for(let agenda of dataAgenda) {%>
                                            <tr>
                                                <th scope="row" class="text-center align-middle">
                                                    <%= agenda.date %>
                                                </th>
                                                <td class="text-center align-middle">
                                                    <%= agenda.month %>
                                                </td>
                                                <td class="text-center align-middle">
                                                    <%= agenda.time %>
                                                </td>
                                                <td class="text-center align-middle">
                                                    <%= agenda.dailyAgenda %>
                                                </td>
                                                <td class="text-center align-middle">
                                                    <%= agenda.category %>
                                                </td>
                                                <td class="text-center align-middle">
                                                    <% if(agenda.hasil=='DONE' ) {%>
                                                        <a href="/dashboard/status/done"><button class="btn-success">
                                                                <%= agenda.hasil %>
                                                            </button>
                                                        </a>
                                                        <% }else if(agenda.hasil=='DELAY' ) {%>
                                                            <a href="/dashboard/status/delay"><button
                                                                    class="btn-primary delay">
                                                                    <%= agenda.hasil %>
                                                                </button>
                                                            </a>
                                                            <% }else if(agenda.hasil=='CANCEL' ) {%>
                                                                <a href="/dashboard/status/cancel"><button
                                                                        class="btn-danger">
                                                                        <%= agenda.hasil %>
                                                                    </button>
                                                                </a>
                                                                <% }else if(agenda.hasil=='NOT YET' ) {%>
                                                                    <a href="/dashboard/status/notyet"><button
                                                                            class="btn-secondary">
                                                                            <%= agenda.hasil %>
                                                                        </button>
                                                                    </a>
                                                                    <% }else if(agenda.hasil=='ON-GOING' ) {%>
                                                                        <a href="/dashboard/status/ongoing"><button
                                                                                class="btn-warning">
                                                                                <%= agenda.hasil %>
                                                                            </button>
                                                                        </a>
                                                                        <% } %>
                                                </td>


                                                <% if(IsAdmin !='guest' ) {%>
                                                    <td class="text-center">
                                                        <!-- <div class="d-flex"> -->
                                                        <a href="/dashboard/edit/<%= agenda.id %>"><button
                                                                class="btn btn-info">Edit</button></a>
                                                        <!-- <div class="invisible">...</div> -->
                                                    </td>
                                                    <td class="text-center">

                                                        <form action="/dashboard/<%= agenda.id %>?_method=delete"
                                                            method="post">

                                                            <button type="button" class="btn btn-danger"
                                                                data-bs-toggle="modal"
                                                                data-bs-target="#exampleModal<%= agenda.id %>">Hapus</button>

                                                            <!-- Modal -->
                                                            <div class="modal fade" id="exampleModal<%= agenda.id %>"
                                                                tabindex="-1"
                                                                aria-labelledby="exampleModalLabel<%= agenda.id %> "
                                                                aria-hidden="true">
                                                                <div class="modal-dialog">
                                                                    <div class="modal-content">
                                                                        <div class="modal-header">
                                                                            <h5 class="modal-title"
                                                                                id="exampleModalLabel<%= agenda.id %> ">
                                                                                Modal title</h5>
                                                                            <button type="button" class="btn-close"
                                                                                data-bs-dismiss="modal"
                                                                                aria-label="Close"></button>
                                                                        </div>

                                                                        <div class="modal-footer">
                                                                            <button type="button"
                                                                                class="btn btn-secondary"
                                                                                data-bs-dismiss="modal">Batalkan</button>
                                                                            <button type="submit"
                                                                                class="btn btn-danger">Tetap
                                                                                Hapus</button>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>

                                                        </form>
                                                    </td>
                                                    <% } %>
        </div>

        </tr>
        <% } %>
            </tbody>
            </table>
            <% } %>

