            <div class="container">
                <div class="row">
                    <div class="col d-flex-inline">
                        <div class="btn-group">
                            <button type="button" class="btn btn-primary dropdown-toggle" data-bs-toggle="dropdown"
                                aria-expanded="false">
                                TAHUN
                            </button>
                            <ul class="dropdown-menu">

                                <li><a class="dropdown-item" href="/dashboard/year/2022">2022</a></li>
                            </ul>
                        </div>



                        <div class="btn-group">
                            <button type="button" class="btn btn-primary dropdown-toggle" data-bs-toggle="dropdown"
                                aria-expanded="false">
                                BULAN
                            </button>
                            <ul class="dropdown-menu">

                                <li><a class="dropdown-item" href="/dashboard/month/januari">Januari</a></li>
                                <li><a class="dropdown-item" href="/dashboard/month/februari">Februari</a></li>
                                <li><a class="dropdown-item" href="/dashboard/month/maret">Maret</a></li>
                                <li><a class="dropdown-item" href="/dashboard/month/april">April</a></li>
                                <li><a class="dropdown-item" href="/dashboard/month/mei">Mei</a></li>
                                <li><a class="dropdown-item" href="/dashboard/month/juni">Juni</a></li>
                                <li><a class="dropdown-item" href="/dashboard/month/juli">Juli</a></li>
                                <li><a class="dropdown-item" href="/dashboard/month/agustus">Agustus</a></li>
                                <li><a class="dropdown-item" href="/dashboard/month/september">September</a></li>
                                <li><a class="dropdown-item" href="/dashboard/month/oktober">Oktober</a></li>
                                <li><a class="dropdown-item" href="/dashboard/month/november">November</a></li>
                                <li><a class="dropdown-item" href="/dashboard/month/desember">Desember</a></li>
                            </ul>
                        </div>

                        <!-- Status -->


                    </div>
                </div>
            </div>



        <div class="container">
            <div class="row">
                <div class="col mt-3 d-flex-inline">
                    <a href="/dashboard/status/done"><button type="button" class="btn btn-success">DONE</button></a>
                        <a href="/dashboard/status/ongoing"><button type="button"
                                class="btn btn-warning">ON-GOING</button></a>
                        <a href="/dashboard/status/notyet"><button type="button" class="btn btn-secondary">NOT YET</button></a>
                        <a href="/dashboard/status/delay"><button type="button" class="btn btn-primary">DELAY</button></a>
                        <a href="/dashboard/status/cancel"><button type="button" class="btn btn-danger">CANCEL</button></a>
                </div>
            </div>
        </div>


        <p class="mt-4"><a href="/dashboard/new">Tambah Agenda Baru</a></p>
        <% if(success && success.length) {%>
            <div class="alert alert-success alert-dismissible fade show" role="alert">
                <%= success %>
                    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>
            <% } %>
                <% if(dataAgenda) {%>
                    <table
                        class="table table-dark table-striped table-bordered border-primary table-sm mb-5 table-hover css-serial">
                        <thead>
                            <tr>
                                <th scope="col" class="text-center align-middle">Date</th>
                                <th scope="col" class="text-center align-middle">Month</th>
                                <th scope="col" class="text-center align-middle">Time</th>
                                <th scope="col" class="text-center align-middle">Daily Agenda</th>
                                <th scope="col" class="text-center align-middle">Category</th>
                                <th scope="col" class="text-center align-middle">STATUS</th>
                                <th scope="col" class="text-center align-middle">Katerangan Hasil</th>
                                <th scope="col" class="text-center align-middle">Katerangan Final</th>
                                <th scope="col" class="text-center align-middle">Edit</th>
                                <th scope="col" class="text-center align-middle">Hapus</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for(let agenda of dataAgenda) {%>
                                <tr>
                                    <th scope="row" class="text-center align-middle">
                                        <%= agenda.date %>
                                    </th>
                                    <td class="text-center align-middle">
                                        <%= agenda.month %>
                                    </td>
                                    <td class="text-center align-middle">
                                        <%= agenda.time %>
                                    </td>
                                    <td class="text-center align-middle">
                                        <%= agenda.dailyAgenda %>
                                    </td>
                                    <td class="text-center align-middle">
                                        <%= agenda.category %>
                                    </td>
                                    <td class="text-center align-middle">
                                        <% if(agenda.hasil=='DONE' ) {%>
                                            <a href="/dashboard/status/done"><button class="btn-success">
                                                <%= agenda.hasil %>
                                            </button>
                                        </a>
                                            <% }else if(agenda.hasil=='DELAY' ) {%>
                                                <a href="/dashboard/status/delay"><button class="btn-primary delay">
                                                    <%= agenda.hasil %>
                                                </button>
                                            </a>
                                                <% }else if(agenda.hasil=='CANCEL' ) {%>
                                                    <a href="/dashboard/status/cancel"><button class="btn-danger">
                                                        <%= agenda.hasil %>
                                                        </button>
                                                    </a>
                                                            <% }else if(agenda.hasil=='NOT YET' ) {%>
                                                                <a href="/dashboard/status/notyet"><button class="btn-secondary">
                                                                    <%= agenda.hasil %>
                                                                    </button>
                                                                </a>
                                                                        <% }else if(agenda.hasil=='ON-GOING' ) {%>
                                                                            <a href="/dashboard/status/ongoing"><button class="btn-warning">
                                                                                <%= agenda.hasil %>
                                                                            </button>
                                                                        </a>
                                                                                    <% } %>
                                    </td>
                                    <td class="text-center align-middle">
                                        <%= agenda.kateranganHasil %>
                                    </td>
                                    <td class="text-center align-middle">
                                        <%= agenda.kateranganFinal %>
                                    </td>

                                    <td>
                                        <!-- <div class="d-flex"> -->
                                        <a href="/dashboard/edit/<%= agenda.id %>"><button
                                                class="btn btn-info">Edit</button></a>
                                        <!-- <div class="invisible">...</div> -->
                                    </td>
                                    <td>
                                        <form action="/dashboard/<%= agenda.id %>?_method=delete" method="post">
                                            <button class="btn btn-danger">Hapus</button>
                                        </form>
                                    </td>
    </div>

    </tr>
    <% } %>
        </tbody>
        </table>
        <% } %>

            <!-- Tambah Data Baru -->






            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
                integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
                crossorigin="anonymous"></script>
            <script src="counter.js"></script>


</body>

</html>